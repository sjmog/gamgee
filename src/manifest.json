{
    "name": "gamgee",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "tabs", "scripting"],
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": ["*://*/"],
    "options_page": "options.html",
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
    }
}
